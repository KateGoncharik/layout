{
  "plugins": ["import"],
  "extends": ["eslint:recommended", "plugin:import/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "curly": ["error", "all"],

    "no-debugger": "off",
    "no-console": "off",
    "class-methods-use-this": 2
  },
  "settings": {}
}
